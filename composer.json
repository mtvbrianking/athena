{
    "name": "bmatovu/athena",
    "description": "A mirco PHP framework.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "bmatovu",
            "email": "mtvbrianking@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "aura/router": "^3.1",
        "bmatovu/http-server-middleware": "^1.0",
        "middlewares/aura-router": "^1.1",
        "middlewares/request-handler": "^1.4",
        "middlewares/utils": "^2.1",
        "middlewares/whoops": "^1.2",
        "narrowspark/http-emitter": "^0.7.0",
        "zendframework/zend-diactoros": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Athena\\": "src/",
            "App\\": "example/app"
        }
    },
    "config": {
        "platform": {
          "php": "7.1"
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
